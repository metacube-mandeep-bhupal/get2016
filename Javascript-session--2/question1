<body onload="findGreatest()">


<script type="text/javascript">
function findGreatest()
{
	var num1=prompt("Enter number 1: ", "number 1");
	var num2=prompt("Enter number 2: ", "number 2");
	var num3=prompt("Enter number 3: ", "number 3");
	
	var great=num1;
	if(num2>great)
		great=num2;
	if(num3>great)
		great=num3;
	
	window.alert(great+" is the gr eatest number");
	}

</script>
